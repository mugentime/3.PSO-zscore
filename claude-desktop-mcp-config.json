      ]
    },
    "tradingview": {
      "command": "uv",
      "args": ["tool", "run", "tradingview-chart-mcp"]
    },
    "taskmaster": {
      "command": "uv", 
      "args": [
        "tool", 
        "run", 
        "task-master-ai",
        "--project-root",
        "C:\\Users\\je2al\\Desktop\\3.PSO-zscore"
      ],
      "env": {
        "ANTHROPIC_API_KEY": "YOUR_ANTHROPIC_API_KEY_HERE",
        "PERPLEXITY_API_KEY": "YOUR_PERPLEXITY_API_KEY_HERE"
      }
